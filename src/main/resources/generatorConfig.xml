<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC
		"-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<!-- !!!! Driver Class Path !!!! -->
	<classPathEntry location="F:\IntelliJ IDEA 2017.3.5\plugins\maven\lib\maven3\repo\repo\mysql\mysql-connector-java\5.1.38\mysql-connector-java-5.1.38.jar"/>

	<context id="context" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="beginningDelimiter" value="`"/>
			<property name="endingDelimiter" value="`"/>
			<property name="suppressAllComments" value="false"/>
			<property name="addRemarkComments" value="true"/>
			<property name="suppressDate" value="true"/>
		</commentGenerator>

		<!-- !!!! Database Configurations !!!! -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.1.3:3306/meeting_customized" userId="root" password=""/>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false"/>
		</javaTypeResolver>

		<!-- !!!! Model Configurations !!!! -->
		<javaModelGenerator targetPackage="com.chaoxing.basicedu.meeting_customized.model" targetProject="src/main/java">
			<property name="enableSubPackages" value="false"/>
			<property name="trimStrings" value="false"/>
		</javaModelGenerator>

		<!-- !!!! Mapper XML Configurations !!!! -->
		<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
			<property name="enableSubPackages" value="false"/>
		</sqlMapGenerator>

		<!-- !!!! Mapper Interface Configurations !!!! -->
		<javaClientGenerator targetPackage="com.chaoxing.basicedu.meeting_customized.mapper" targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="false"/>
		</javaClientGenerator>

		<!-- !!!! Table Configurations !!!! -->
		<table tableName="%" enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false">
			<generatedKey column="id" sqlStatement="MySql"/>
		</table>
	</context>
</generatorConfiguration>